<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:background="@color/bjmgf_sdk_white"
    android:gravity="center"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/id_question_footer_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:text="@string/bjmgf_sdk_customerCenterPhoneNumerHitStr"
        android:textColor="@color/bjmgf_sdk_textgray" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:text="@string/bjmgf_sdk_customerCenterPhoneNumerStr"
        android:textColor="@color/bjmgf_sdk_textgray" />

</LinearLayout>