<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/bjmgf_sdk_dock_wish_list_item"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <ImageView android:id="@+id/bjmgf_sdk_dock_wish_item_icon"
        android:layout_width="@dimen/bjmgf_sdk_goods_icon_size"
        android:layout_height="@dimen/bjmgf_sdk_goods_icon_size"
       	android:layout_alignLeft="@+id/bjmgf_sdk_dock_wish_item_icon_bg"
		android:layout_alignTop="@+id/bjmgf_sdk_dock_wish_item_icon_bg"/>
        
	<ImageView android:id="@+id/bjmgf_sdk_dock_wish_item_icon_bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:src="@drawable/bjmgf_sdk_dock_wishlist_item_bg"/>

	<LinearLayout android:id="@+id/bjmgf_sdk_dock_wish_item_price"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_centerHorizontal="true"
	    android:layout_below="@id/bjmgf_sdk_dock_wish_item_icon_bg"
	    android:orientation="horizontal">
	    <ImageView android:id="@+id/bjmgf_sdk_dock_wish_item_price_icon"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	    	android:layout_marginRight="@dimen/bjmgf_sdk_margin2"
	        android:src="@drawable/bjmgf_sdk_dock_wish_item_price_icon"/>
	    <TextView android:id="@+id/bjmgf_sdk_dock_wish_item_price_text"
	        android:layout_marginLeft="@dimen/bjmgf_sdk_margin2"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textColor="@color/bjmgf_sdk_text_price_color"
	        android:textSize="@dimen/bjmgf_sdk_font_size14"
	        android:text="5"/>
	</LinearLayout>
	    
    <TextView android:id="@+id/bjmgf_sdk_dock_wish_item_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/bjmgf_sdk_dock_wish_item_price"
        android:textColor="@color/bjmgf_sdk_black"
        android:textSize="@dimen/bjmgf_sdk_font_size12"
        android:singleLine="true"
        android:text="物品1"/>

</RelativeLayout>
